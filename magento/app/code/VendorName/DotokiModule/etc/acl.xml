<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Magento_Backend::marketing">
                    <resource id="VendorName_DotokiModule::aclindex" title="DotokiRole" sortOrder="60">
                        <resource id="VendorName_DotokiModule::index" title="Dotoki Role" />
                        <resource id="VendorName_DotokiModule::index2" title="Dotoki Role 2" />
                        <resource id="VendorName_DotokiModule::subscription" title="Dotoki subscription" />
                    </resource>
                </resource>
                <resource id="Magento_Backend::stores">
                    <resource id="Magento_Backend::stores_settings">
                        <resource id="Magento_Config::config">
                            <resource id="VendorName_DotokiModule::Dotoki_config"  title="Dotoki Section" />
                        </resource>
                    </resource>
                </resource>
                <resource id="VendorName_DotokiModule::dotokimenu" title="DotokiMenu-Role">
                    <resource id="VendorName_DotokiModule::post" title="Dotoki Post Role" />
                    <resource id="VendorName_DotokiModule::configuration" title="Dotoki Configuration Role" />
                </resource>
            </resource>

        </resources>

    </acl>
</config>
